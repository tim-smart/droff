{
  "packages": ["packages/*"],
  "version": "independent",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "version": {
      "allowBranch": "main",
      "conventionalCommits": true,
      "changelogPreset": "angular",
      "message": "chore(release): %s",
      "createRelease": "github"
    }
  }
}
