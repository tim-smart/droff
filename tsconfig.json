{
  "compilerOptions": {
    "declaration": true,
    "downlevelIteration": true,
    "lib": ["ESNext"],
    "target": "ESNext",
    "moduleResolution": "node",
    "module": "CommonJS",
    "isolatedModules": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "removeComments": false,
    "importHelpers": true
  }
}
